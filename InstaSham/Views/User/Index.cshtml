@model IEnumerable<User>
<h1>All Users</h1>
<table class="table">
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Username</th>
			<th>View Profile</th>
			<th>Edit User</th>
			<th>Delete User</th>
		</tr>
	</thead>
	<tbody>
		@foreach (User user in Model)
		{
			<tr>
				<td>@user.FirstName</td>
				<td>@user.LastName</td>
				<td>@user.Username</td>
				<td>
					<a href="/User/Profile/@user.Id">Profile</a>
				</td>
				<td>
					<a href="/Edit/@user.Id">Edit</a>
				</td>
				<td>
					<a href="/Delete/@user.Id">Delete</a>
				</td>
			</tr>
		}
	</tbody>
</table>
<a href="/Suspect/Create">Create New User</a>